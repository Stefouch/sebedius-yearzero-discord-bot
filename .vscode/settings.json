{
  "i18n-ally.enabledFrameworks": ["i18next", "custom"],
  "i18n-ally.keystyle": "nested",
  "i18n-ally.localesPaths": [
    "src/locales",
  ],
  "i18n-ally.namespace": true,
  "i18n-ally.pathMatcher": "{locale}/{namespace}.yml",
  "i18n-ally.sourceLanguage": "en-US",
  "i18n-ally.usage.derivedKeyRules": [
    "{key}_one",
    "{key}_two",
    "{key}_three",
    "{key}_few",
    "{key}_many",
    "{key}_other",
    "{key}_plural"
  ],
}
